#palindrome in python
num=int(input("enter anumber:")
rev=0
rem=0
while(num!=0):
num=num%10
rev=rev*10+rem
num=num/10
if(num==rev)
print("it is palkindrome")
else
print("not a palindrome")
      
#armstrong number
num=int(input("enter anumber:")
sum=0
rem=0
while(num!=0):
num=num%10
sum=sum+rem*rem*rem
num=num/10
if(num==sum)
print("it is armstrong")
else
print("not a armstrong")
